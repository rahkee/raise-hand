{
	"name": "raise-hand",
	"displayName": "Raise Hand",
	"description": "Visual notification when AI assistant finishes generating code in Cursor",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
