		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.80.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vscode/vsce": "^3.4.2",
		"eslint": "^8.44.0",
		"typescript": "^5.1.6"
	}
}
